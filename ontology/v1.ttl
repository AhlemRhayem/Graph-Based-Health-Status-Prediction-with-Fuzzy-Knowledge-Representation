@base <https://w3id.org/HealthIoT#> .

<https://w3id.org/HealthIoT> rdf:type owl:Ontology ;
                              owl:versionIRI <https://w3id.org/HealthIoT/1.1.1> .

#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/HealthIoT#degree
:degree rdf:type owl:AnnotationProperty .


###  https://w3id.org/HealthIoT#fuzzyLabel
:fuzzyLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  https://w3id.org/HealthIoT#Adult
:Adult rdf:type rdfs:Datatype ;
       :fuzzyLabel """[
        fuzzy:type \"triangular\" ;
        fuzzy:a \"30\"^^xsd:integer ;
        fuzzy:b \"47\"^^xsd:integer ;
        fuzzy:c \"65\"^^xsd:integer
    ]""" .


###  https://w3id.org/HealthIoT#Old
:Old rdf:type rdfs:Datatype ;
     :fuzzyLabel """[
        fuzzy:type \"triangular\" ;
        fuzzy:a \"60\"^^xsd:integer ;
        fuzzy:b \"75\"^^xsd:integer ;
        fuzzy:c \"85\"^^xsd:integer
    ]""" .


###  https://w3id.org/HealthIoT#Young
:Young rdf:type rdfs:Datatype ;
       :fuzzyLabel """[
        fuzzy:type \"triangular\" ;
        fuzzy:a \"10\"^^xsd:integer ;
        fuzzy:b \"20\"^^xsd:integer ;
        fuzzy:c \"35\"^^xsd:integer
    ]""" .


#################################################################
#    Object Properties
#################################################################

###  https://saref.etsi.org/core#makesMeasurement
<https://saref.etsi.org/core#makesMeasurement> rdf:type owl:ObjectProperty ;
                                               rdfs:domain ssn:Device ;
                                               rdfs:range ssn:Measurement .


###  https://saref.etsi.org/core#relatesToMeasurement
<https://saref.etsi.org/core#relatesToMeasurement> rdf:type owl:ObjectProperty ;
                                                   owl:inverseOf <https://saref.etsi.org/core#relatesToProperty> ;
                                                   rdfs:domain ssn:Property ;
                                                   rdfs:range ssn:Measurement .


###  https://saref.etsi.org/core#relatesToProperty
<https://saref.etsi.org/core#relatesToProperty> rdf:type owl:ObjectProperty ;
                                                rdfs:domain ssn:Measurement ;
                                                rdfs:range ssn:Property .


###  https://w3id.org/HealthIoT#about
:about rdf:type owl:ObjectProperty ;
       rdfs:domain :Alert ;
       rdfs:range :Service .


###  https://w3id.org/HealthIoT#alcoholConsumption
:alcoholConsumption rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :relatedTo ;
                    rdfs:domain :Patient ;
                    rdfs:range :AlcoholConsumption .


###  https://w3id.org/HealthIoT#analysis
:analysis rdf:type owl:ObjectProperty ;
          rdfs:domain :MedicalStaff ;
          rdfs:range ssn:Measurement .


###  https://w3id.org/HealthIoT#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:domain :MedicalObject ;
          rdfs:range ssn:Device .


###  https://w3id.org/HealthIoT#generates
:generates rdf:type owl:ObjectProperty ;
           rdfs:domain ssn:Device ;
           rdfs:range :Alert .


###  https://w3id.org/HealthIoT#hasMeasurement
:hasMeasurement rdf:type owl:ObjectProperty ;
                rdfs:domain :Patient ;
                rdfs:range :Measurement .


###  https://w3id.org/HealthIoT#hasScore
:hasScore rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :Patient ;
          rdfs:range :HealthScore .


###  https://w3id.org/HealthIoT#isMeasuredIn
:isMeasuredIn rdf:type owl:ObjectProperty ;
              rdfs:domain ssn:Measurement ;
              rdfs:range <http://www.ontology-of-units-of-measure.org/resource/om-2#UnitMeasurement> .


###  https://w3id.org/HealthIoT#isPropertyOf
:isPropertyOf rdf:type owl:ObjectProperty ;
              rdfs:domain ssn:Property ;
              rdfs:range ssn:FeatureOfInterest .


###  https://w3id.org/HealthIoT#medicalCondition
:medicalCondition rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :relatedTo ;
                  rdfs:domain :Patient ;
                  rdfs:range :Disease .


###  https://w3id.org/HealthIoT#mood
:mood rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :relatedTo ;
      rdfs:domain :Patient ;
      rdfs:range :Mood .


###  https://w3id.org/HealthIoT#observes
:observes rdf:type owl:ObjectProperty ;
          rdfs:domain ssn:Device ;
          rdfs:range ssn:Property .


###  https://w3id.org/HealthIoT#produces
:produces rdf:type owl:ObjectProperty ;
          rdfs:domain :MedicalStaff ;
          rdfs:range :Service .


###  https://w3id.org/HealthIoT#relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Context ;
           rdfs:range :Patient ;
           rdfs:label "related to" .


###  https://w3id.org/HealthIoT#stressLevel
:stressLevel rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :relatedTo ;
             rdfs:domain :Patient ;
             rdfs:range :Symptom .


###  https://w3id.org/HealthIoT#wears
:wears rdf:type owl:ObjectProperty ;
       rdfs:domain ssn:FeatureOfInterest ;
       rdfs:range ssn:Device .


