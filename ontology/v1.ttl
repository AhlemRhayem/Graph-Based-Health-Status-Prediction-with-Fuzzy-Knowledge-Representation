###  https://w3id.org/HealthIoT#height
:height rdf:type owl:DatatypeProperty ;
        rdfs:domain :Patient ;
        rdfs:range xsd:float .


###  https://w3id.org/HealthIoT#keywords
:keywords rdf:type owl:DatatypeProperty ;
          rdfs:domain :MedicalObject ;
          rdfs:range xsd:string ;
          rdfs:comment "some keywords that describes a medical object" ;
          rdfs:label "keywords" .


###  https://w3id.org/HealthIoT#manufacturer
:manufacturer rdf:type owl:DatatypeProperty ;
              rdfs:domain :MedicalObject ;
              rdfs:range xsd:string .


###  https://w3id.org/HealthIoT#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain :MedicalObject ,
                  :Patient ;
      rdfs:range xsd:string .


###  https://w3id.org/HealthIoT#patientId
:patientId rdf:type owl:DatatypeProperty ;
           rdfs:domain :Patient ;
           rdfs:range xsd:string ;
           rdfs:comment "The unique identifier of  a patient" ;
           rdfs:label "patient Id" .


###  https://w3id.org/HealthIoT#residenceType
:residenceType rdf:type owl:DatatypeProperty ;
               rdfs:label "residence type" .


###  https://w3id.org/HealthIoT#smokingStatus
:smokingStatus rdf:type owl:DatatypeProperty ;
