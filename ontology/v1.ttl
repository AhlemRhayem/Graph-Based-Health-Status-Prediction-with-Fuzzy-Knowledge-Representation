          rdfs:domain ssn:Device ,
                      :MedicalObject ;
          rdfs:range xsd:string ;
          rdfs:comment "the identifier of a Medical Object" ;
          rdfs:label "device id" .


###  https://w3id.org/HealthIoT#exerciceAngina
:exerciceAngina rdf:type owl:DatatypeProperty ;
                rdfs:domain :PatientLifeStyle ;
                rdfs:label "exercise angina" .


###  https://w3id.org/HealthIoT#gender
:gender rdf:type owl:DatatypeProperty ;
        rdfs:domain :Patient ;
        rdfs:range xsd:string ;
        rdfs:label "gender" .


