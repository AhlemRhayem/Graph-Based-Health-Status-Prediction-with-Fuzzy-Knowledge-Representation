        rdfs:range xsd:float .


#################################################################
#    Classes
#################################################################

###  http://hl7.org/fhir/Medication
fhir:Medication rdf:type owl:Class ;
                rdfs:subClassOf :Treatment .


###  http://www.ontology-of-units-of-measure.org/resource/om-2#UnitMeasurement
<http://www.ontology-of-units-of-measure.org/resource/om-2#UnitMeasurement> rdf:type owl:Class ;
                                                                            rdfs:comment "Defines the unit of each mesurement dtected by a sensor" ;
                                                                            rdfs:label "Unit Measurement" .


###  http://www.w3.org/ns/ssn/Device
ssn:Device rdf:type owl:Class ;
           rdfs:comment "represents the logical objects, which are embedded in the medical object in order to ease the task of data detection and the interaction with the environment." ;
           rdfs:label "Device" .


###  http://www.w3.org/ns/ssn/FeatureOfInterest
ssn:FeatureOfInterest rdf:type owl:Class ;
                      rdfs:comment "represents the real world entities which are monitored and observed by the connected object. In the context of the healthcare domain, this class corresponds to the patient" ;
                      rdfs:label "Feature Of Interest" .


###  http://www.w3.org/ns/ssn/Measurement
ssn:Measurement rdf:type owl:Class ;
                rdfs:comment "class represents the data stream captured from the connected objects." ;
                rdfs:label "Measurement" .


###  http://www.w3.org/ns/ssn/Property
ssn:Property rdf:type owl:Class ;
             rdfs:label "Property" .


###  http://www.w3.org/ns/ssn/SensingDevice
ssn:SensingDevice rdf:type owl:Class ;
                  rdfs:subClassOf ssn:Device ;
                  rdfs:label "Sensing Device" .


###  https://w3id.org/HealthIoT#ActuatingDevice
:ActuatingDevice rdf:type owl:Class ;
                 rdfs:subClassOf ssn:Device ;
                 rdfs:comment "A device that causes a machine or other device to operate." ;
                 rdfs:label "Actuating Device" .


###  https://w3id.org/HealthIoT#Actuator
:Actuator rdf:type owl:Class ;
          rdfs:subClassOf sosa:Actuator ;
          rdfs:comment "A device that causes a machine or other device to operate." ;
          rdfs:label "Actuator" .


###  https://w3id.org/HealthIoT#AdultPatient
:AdultPatient rdf:type owl:Class ;
              rdfs:subClassOf :Patient .


###  https://w3id.org/HealthIoT#AlcoholConsumption
:AlcoholConsumption rdf:type owl:Class ;
                    rdfs:subClassOf :PatientLifeStyle .


###  https://w3id.org/HealthIoT#Alert
:Alert rdf:type owl:Class ;
       rdfs:comment """contains different categories of alerts that can be generated by
the actuator device. These alerts can be for objects management or for patient monitoring (treatment adjsutement, emergency call for ambulance).""" ;
       rdfs:label "Alert" .


###  https://w3id.org/HealthIoT#BloodOxygen
:BloodOxygen rdf:type owl:Class ;
             rdfs:subClassOf :VitalSigns .


###  https://w3id.org/HealthIoT#BodyFat
:BodyFat rdf:type owl:Class ;
         rdfs:subClassOf :VitalSigns ;
         rdfs:comment "Body fat percentage is how much the body is made up of fat vs. bone and muscle." ;
         rdfs:label "Body Fat" .


###  https://w3id.org/HealthIoT#CalorieIntake
:CalorieIntake rdf:type owl:Class ;
               rdfs:subClassOf :HealthProperty ;
               rdfs:label "Calorie Intake" .


###  https://w3id.org/HealthIoT#Capability
:Capability rdf:type owl:Class ;
            rdfs:comment "presents the different characteristics and properties of the medical connected objects and their embedded devices." ;
            rdfs:label "Capability" .


###  https://w3id.org/HealthIoT#ConfigurationService
:ConfigurationService rdf:type owl:Class ;
                      rdfs:subClassOf :Service ;
                      rdfs:comment "contains the state of the connected objects and the proposed solution." ;
                      rdfs:label "Configuration Service" .


###  https://w3id.org/HealthIoT#Context
:Context rdf:type owl:Class ;
         rdfs:comment """characterizes the MCOs and the obtained data about the monitored patient. In fact, the state of both MCOs and the patient’s
health changes continuously according to several factors""" ;
         rdfs:label "Context" .


###  https://w3id.org/HealthIoT#DeepSleepDuration
:DeepSleepDuration rdf:type owl:Class ;
                   rdfs:subClassOf :SleepDuration ;
                   rdfs:label "Deep Sleep Duration" .


###  https://w3id.org/HealthIoT#DeviceProperty
:DeviceProperty rdf:type owl:Class ;
                rdfs:subClassOf ssn:Property ;
                rdfs:comment "Determines the possible property status of a device. which can be available, unavailable, stop, move, shareable, unshareable, etc." ;
                rdfs:label "Device Property" .


###  https://w3id.org/HealthIoT#Diagnosis
:Diagnosis rdf:type owl:Class ;
           rdfs:subClassOf :Service ;
           rdfs:label "Diagnosis" .


###  https://w3id.org/HealthIoT#Disease
:Disease rdf:type owl:Class ;
         rdfs:subClassOf :Context ;
         rdfs:comment "represents a chronic or an ordinary disease that may infect a person. It" ;
         rdfs:label "Disease" .


###  https://w3id.org/HealthIoT#Doctor
:Doctor rdf:type owl:Class ;
        rdfs:subClassOf :MedicalStaff .


###  https://w3id.org/HealthIoT#HealthProperty
:HealthProperty rdf:type owl:Class ;
                rdfs:subClassOf ssn:Property ;
                rdfs:comment "presents the possible heath property that are detected by the medical object for a patient" ;
                rdfs:label "Health Property" .


